@if (TempData["Success"]!=null)
{
    <script>
        Swal.fire('تایید', '@TempData["Success"]', 'success');

    </script>
}
@if (TempData["Error"] != null)
{
    <script>
        Swal.fire('خطا!','@TempData["Error"]','error');
     </script>
}

@if (TempData["Info"] != null)
{
    <script>
         Swal.fire('اعلان!', '@TempData["Info"]', 'info');
    </script>
}

@if (TempData["Warning"] != null)
{
    <script>
        Swal.fire('هشدار', '@TempData["Warning"]', 'warning');
    </script>
}